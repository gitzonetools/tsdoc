{
  "name": "@gitzone/tsdoc",
  "version": "1.0.20",
  "private": false,
  "description": "a tool for better documentation",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "Lossless GmbH",
  "license": "MIT",
  "bin": {
    "tsdoc": "cli.js"
  },
  "scripts": {
    "test": "(tstest test/) && (node ./cli.ts.js) && rm -rf public/",
